#navbar.overlay.overlay-transition-slide
    .links
        ul
            li.home
                a(href='#first-look') Left Goat

            for link in [ 'Who We Are', 'Services', 'Team', 'Our Work' ]
                li
                    a(href='#' + link.toLowerCase().split(' ').join('-'))= link

            li.contact
                a Contact

    a.small-screen-open-button
        svg(viewBox="0 0 32 32")
            line(x1="6" y1="11" x2="26" y2="11")
            line(x1="6" y1="16" x2="26" y2="16")
            line(x1="6" y1="21" x2="26" y2="21")

    a.small-screen-close-button
        svg(viewBox="0 0 25 25")
            line(x1="0" y1="0" x2="25" y2="25")
            line(x1="0" y1="25" x2="25" y2="0")